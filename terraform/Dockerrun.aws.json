{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "auth0-proxy",
            "image": "quintilesims/auth0-proxy:latest",
            "essential": true,
            "memory": 128,
            "environment": [
                {
                    "name": "AP_PROXY_HOST",
                    "value": "${proxy_host}"
                },
                {
                    "name": "AP_PROXY_PORT",
                    "value": "${proxy_port}"
                },
                {
                    "name": "AP_PROXY_SCHEME",
                    "value": "${proxy_scheme}"
                },
                {
                    "name": "AP_AUTH0_DOMAIN",
                    "value": "${auth0_domain}"
                },
                {
                    "name": "AP_AUTH0_CLIENT_ID",
                    "value": "${auth0_client_id}"
                },
                {
                    "name": "AP_AUTH0_CLIENT_SECRET",
                    "value": "${auth0_client_secret}"
                },
                {
                    "name": "AP_AUTH0_REDIRECT_URI",
                    "value": "${auth0_redirect_uri}"
                },
                {
                    "name": "AP_SESSION_SECRET",
                    "value": "${session_secret}"
                },
                {
                    "name": "AP_SESSION_TIMEOUT",
                    "value": "${session_timeout}"
                }
            ],
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}
